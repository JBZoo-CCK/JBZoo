<?xml version="1.0" encoding="utf-8"?>
<element type="element" group="Core" hidden="true">
    <name>JBCart Element - Shipping Core</name>
    <author>JBZoo.com</author>
    <creationDate>2014</creationDate>
    <copyright>Copyright (C) JBZoo.com</copyright>
    <authorEmail>admin@jbzoo.com</authorEmail>
    <authorUrl>http://jbzoo.com</authorUrl>
    <version>1.0</version>
    <description>JBZoo Cart Element Core System</description>
    <params>
        <param name="name" type="text" default="" label="JBZOO_ELEMENT_NAME" description="JBZOO_ELEMENT_NAME_DESC"/>

        <param name="description" type="text" default="" label="JBZOO_ELEMENT_DESCRIPTION"
               description="JBZOO_ELEMENT_DESCRIPTION_DESC"/>

        <param name="access" type="zooaccesslevel" default="" label="JBZOO_ELEMENT_ACCESS"
               description="JBZOO_ELEMENT_ACCESS_DESC"/>

        <param name="currency" type="jbcurrency" default="eur" label="JBZOO_SHIPPING_CURRENCY"
               description="JBZOO_SHIPPING_CURRENCY_DESC"/>

        <param name="modifytotal" type="jbbool" default="0" label="JBZOO_SHIPPING_MODIFYTOTAL"
               description="JBZOO_SHIPPING_MODIFYTOTAL_DESC"/>

        <param name="shippingfields" type="jbcartfields" group="CONFIG_SHIPPINGFIELDS" position="DEFAULT_POSITION"
               multiple="true" label="JBZOO_SHIPPING_SHIPPINGFIELDS_LABEL" desc="JBZOO_SHIPPING_SHIPPINGFIELDS_DESC"/>
    </params>
</element>
