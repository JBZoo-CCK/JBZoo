<?xml version="1.0" encoding="utf-8"?><!--    JBZoo App is universal Joomla CCK, application for YooTheme Zoo component    @package     jbzoo    @version     2.x Pro    @author      JBZoo App http://jbzoo.com    @copyright   Copyright (C) JBZoo.com,  All rights reserved.    @license     http://jbzoo.com/license-pro.php JBZoo Licence    @coder       Denis Smetannikov <denis@jbzoo.com>--><element>    <params group="render">        <param name="jbzoo_filter_render" type="list" label="JBZOO_ELEMENT_TEMPLATE"               description="JBZOO_ELEMENT_TEMPLATE_DESC" default="jbpriceadvance">            <option value="jbpriceadvance">JBZOO_ELEMENT_TEMPLATE_JBPRICEADVANCE</option>        </param>        <param name="jbzoo_filter_default" type="text" default="" label="JBZOO_FILTER_DEFAULT"               description="JBZOO_FILTER_DEFAULT_DESC"/>        <!-- VALUE -->        <param name="@spacer_value" type="jbspacer" default="JBZOO_FILTER_JBPRICE_VALUE"/>        <param name="jbzoo_filter_value" type="list" label="JBZOO_FILTER_JBPRICE_TEMPLATE_VALUE"               description="JBZOO_FILTER_JBPRICE_TEMPLATE_VALUE_DESC" default="3">            <option value="0">JBZOO_FILTER_JBPRICE_TEMPLATE_NONE</option>            <option value="1">JBZOO_FILTER_JBPRICE_TEMPLATE_SIMPLE</option>            <option value="2">JBZOO_FILTER_JBPRICE_TEMPLATE_RANGE</option>            <option value="3">JBZOO_FILTER_JBPRICE_TEMPLATE_SLIDER</option>        </param>        <param name="jbzoo_filter_value_type" type="radio" label="JBZOO_FILTER_JBPRICE_VALUE_TYPE"               description="JBZOO_FILTER_JBPRICE_VALUE_TYPE_DESC" default="0">            <option value="0">JBZOO_FILTER_JBPRICE_VALUE_TYPE_ALL</option>            <option value="1">JBZOO_FILTER_JBPRICE_VALUE_TYPE_PRICE</option>            <option value="2">JBZOO_FILTER_JBPRICE_VALUE_TYPE_TOTAL</option>        </param>        <param name="jbzoo_filter_price_type" type="radio" label="JBZOO_FILTER_JBPRICE_PRICE_TYPE"               description="JBZOO_FILTER_JBPRICE_PRICE_TYPE_DESC" default="0">            <option value="0">JBZOO_FILTER_JBPRICE_PRICE_TYPE_ALL</option>            <option value="1">JBZOO_FILTER_JBPRICE_PRICE_TYPE_PRIMARY</option>            <option value="2">JBZOO_FILTER_JBPRICE_PRICE_TYPE_SECONDARY</option>        </param>        <param name="jbzoo_filter_currency_default" type="jbcurrency" default="EUR"               label="JBZOO_FILTER_JBPRICE_CURRENCY_DEFAULT"               description="JBZOO_FILTER_JBPRICE_CURRENCY_DEFAULT_DESC" multiple="0"/>        <!-- jquery.ui slider -->        <param name="@spacer_slider" type="jbspacer" default="JBZOO_SLIDER_ONLY"/>        <param name="jbzoo_filter_slider_auto" type="jbbool" default="0" label="JBZOO_FILTER_SLIDER_AUTO"               description="JBZOO_FILTER_SLIDER_AUTO_DESC"/>        <param name="jbzoo_filter_slider_min" type="text" default="0" label="JBZOO_FILTER_SLIDER_MIN"               description="JBZOO_FILTER_SLIDER_MIN_DESC"/>        <param name="jbzoo_filter_slider_max" type="text" default="10000" label="JBZOO_FILTER_SLIDER_MAX"               description="JBZOO_FILTER_SLIDER_MAX_DESC"/>        <param name="jbzoo_filter_slider_step" type="text" default="100" label="JBZOO_FILTER_SLIDER_STEP"               description="JBZOO_FILTER_SLIDER_STEP_DESC"/>        <!-- SKU -->        <param name="@spacer_sku" type="jbspacer" default="JBZOO_FILTER_JBPRICE_SKU"/>        <param name="jbzoo_filter_sku" type="jbbool" default="1" label="JBZOO_FILTER_JBPRICE_SKU_ENABLED"               description="JBZOO_FILTER_JBPRICE_SKU_ENABLED"/>        <param name="jbzoo_filter_placeholder" type="text" default="" label="JBZOO_FILTER_PLACEHOLDER"               description="JBZOO_FILTER_PLACEHOLDER_DESC"/>        <param name="jbzoo_filter_autocomplete" type="jbbool" default="1" label="JBZOO_FILTER_AUTOCOMPLETE"               description="JBZOO_FILTER_AUTOCOMPLETE_DESC"/>        <!-- OTHER -->        <param name="@spacer_other" type="jbspacer" default="JBZOO_FILTER_JBPRICE_OTHER"/>        <param name="jbzoo_filter_balance" type="jbbool" default="1" label="JBZOO_FILTER_JBPRICE_BALANCE_ENABLED"               description="JBZOO_FILTER_JBPRICE_BALANCE_ENABLED_DESC"/>        <param name="jbzoo_filter_new" type="jbbool" default="1" label="JBZOO_FILTER_JBPRICE_NEW_ENABLED"               description="JBZOO_FILTER_JBPRICE_NEW_ENABLED_DESC"/>        <param name="jbzoo_filter_hit" type="jbbool" default="1" label="JBZOO_FILTER_JBPRICE_HIT_ENABLED"               description="JBZOO_FILTER_JBPRICE_HIT_ENABLED_DESC"/>        <param name="jbzoo_filter_sale" type="jbbool" default="1" label="JBZOO_FILTER_JBPRICE_SALE_ENABLED"               description="JBZOO_FILTER_JBPRICE_SALE_ENABLED_DESC"/>    </params></element>